/**
 * Test-only wrangler configuration
 * Excludes AI and Vectorize bindings that require remote authentication
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "rafters-api-test",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-06",
  "queues": {
    "producers": [
      {
        "queue": "color-processing-queue",
        "binding": "COLOR_SEED_QUEUE"
      }
    ],
    "consumers": [
      {
        "queue": "color-processing-queue"
      }
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "rafters-test",
      "database_id": "test-db-id"
    }
  ]
}
